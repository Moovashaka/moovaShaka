{
  "backend": {
    "name": "github",
    "repo": "Moovashaka/moovaShaka",
    "branch": "cmsAdd"
  },
  "display_url": "https://moovashaka.co.uk",
  "media_folder": "public/img/uploads",
  "public_folder": "/img/uploads",
  "collections": [
    {
      "name": "posts",
      "label": "Post",
      "description": "The description is a great place for tone setting, high level information, and editing guidelines that are specific to a collection.\n",
      "folder": "public/_posts",
      "slug": "{{year}}-{{month}}-{{day}}-{{slug}}",
      "create": true,
      "fields": [
        {
          "label": "Title",
          "name": "title",
          "widget": "string",
          "tagname": "h1"
        },
        {
          "label": "Publish Date",
          "name": "date",
          "widget": "datetime",
          "format": "YYYY-MM-DD hh:mma"
        },
        {
          "label": "Cover Image",
          "name": "image",
          "widget": "image",
          "required": false,
          "tagname": ""
        },
        {
          "label": "Body",
          "name": "body",
          "widget": "markdown"
        }
      ],
      "meta": [
        {
          "label": "SEO Description",
          "name": "description",
          "widget": "text"
        }
      ]
    },
    {
      "name": "faq",
      "label": "FAQ",
      "folder": "public/_faqs",
      "create": true,
      "fields": [
        {
          "label": "Question",
          "name": "title",
          "widget": "string",
          "tagname": "h1"
        },
        {
          "label": "Answer",
          "name": "body",
          "widget": "markdown"
        }
      ]
    },
